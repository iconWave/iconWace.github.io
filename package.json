{
  "name": "vite-ts-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "dev:development": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "build:production": "vue-tsc --noEmit && vite build --mode production",
    "build:check": "vue-tsc --noEmit && vite build",
    "build:github": "vite build --base=/vite-vue3-ts/",
    "preview": "vite preview",
    "eslint:comment": "使用 ESLint 检查并自动修复 src 目录下所有扩展名为 .js 和 .vue 的文件",
    "eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "prettier:comment": "自动格式化当前目录下的所有文件",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "commit:comment": "引导设置规范化的提交信息",
    "commit":"git-cz"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "dayjs": "^1.11.4",
    "echarts": "^5.3.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.16",
    "sanitize.css": "^13.0.0",
    "unplugin-auto-import": "^0.10.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-request": "^1.2.4",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@faker-js/faker": "^7.4.0",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "rollup": "^2.0.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.38.9"
  },
  "config": {
      "commitizen": {
        "path": "node_modules/cz-customizable"
      }
  }
}